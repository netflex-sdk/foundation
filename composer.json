{
    "name": "netflex/foundation",
    "description": "Netflex Foundation library",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/translation": "^12.0",
        "netflex/api": "^6.0",
        "netflex/support": "^6.0",
        "justinrainbow/json-schema": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Foundation\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "require-dev": {
        "illuminate/cache": "^12.0",
        "illuminate/container": "^12.0",
        "illuminate/filesystem": "^12.0",
        "larapack/dd": "^1.1",
        "vlucas/phpdotenv": "^5.2",
        "php": "^8.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Foundation\\Providers\\FoundationServiceProvider",
                "Netflex\\Foundation\\Providers\\TranslationServiceProvider"
            ]
        }
    }
}
