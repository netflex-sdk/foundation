{
  "name": "netflex/foundation",
  "description": "Netflex Foundation library",
  "type": "library",
  "license": "MIT",
  "keywords": [],
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.3",
    "illuminate/translation": "^8.0",
    "netflex/api": "^3.0",
    "netflex/support": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Netflex\\Foundation\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "illuminate/cache": "^8.0",
    "illuminate/container": "^8.0",
    "illuminate/filesystem": "^8.0",
    "larapack/dd": "^1.1",
    "vlucas/phpdotenv": "^3.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Netflex\\Foundation\\Providers\\TranslationServiceProvider"
      ]
    }
  }
}
